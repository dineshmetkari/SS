package com.stackroute.redisson;

import java.util.ArrayList;
import java.util.Map;

import org.springframework.stereotype.Component;

@Component
public class TermBankSynonyms {
	
	
	private Map<String,ArrayList<Map<String,ArrayList<String>>>>  synoMap;

	public Map<String, ArrayList<Map<String, ArrayList<String>>>> getSynoMap() {
		return synoMap;
	}

	public void setSynoMap(Map<String, ArrayList<Map<String, ArrayList<String>>>> synoMap) {
		this.synoMap = synoMap;
	}
	

}
